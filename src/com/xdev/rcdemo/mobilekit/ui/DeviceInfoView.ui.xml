<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE xml>

<?xdevuiml version="1.0"?>
<?template APPLE_IPHONE_6?>
<?import com.xdev.rcdemo.mobilekit.ui.NavBar?>
<?import com.xdev.ui.XdevGridLayout?>
<?import com.xdev.ui.XdevLabel?>
<?import com.xdev.ui.XdevView?>
<XdevView xmlns:x="http://xml.xdev-software.com/xdevuiml" x:name="DeviceInfoView">
    <x:constraints zpos="0" width="100%" height="100%" />
    <x:event>attach.attach</x:event>
    <XdevGridLayout x:name="gridLayout">
        <x:constraints zpos="0" width="100%" height="100%" />
        <NavBar title="Device Info" x:name="navBar">
            <x:constraints gridx="1" gridy="1" weighty="0.0" weightx="0.1" width="100%" />
        </NavBar>
        <XdevLabel x:name="lblResult" value="Result" contentMode="com.vaadin.shared.ui.label.ContentMode.PREFORMATTED">
            <x:constraints gridx="1" gridy="2" weighty="0.0" weightx="0.1" />
        </XdevLabel>
    </XdevGridLayout>
</XdevView>

